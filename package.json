{
  "name": "aastar-start",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:backend": "cd packages/fake-backend && pnpm start",
    "dev:frontend": "cd packages/coinjar-demo && pnpm run dev",
    "dev:sdk": "cd packages/aastar-sdk && pnpm run build --watch",
    "build:all": "pnpm -r run build",
    "install:all": "pnpm -r install",
    "clean": "pnpm -r run clean && rm -rf node_modules"
  },
  "devDependencies": {
    "pnpm": "^10.14.0"
  }
}
